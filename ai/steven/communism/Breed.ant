#make_love {
    @begin
    turn left
    turn left
    call #safe_move @return
    turn left
    turn left
    turn left
    // We now look at our food!

    flip 3 else *2
    unset 5

    @waitForBreed
    flip 100 else *2
    jump @return
    breed else @waitForBreed
    move else @return
    pickup else @return
    jump @begin

}
